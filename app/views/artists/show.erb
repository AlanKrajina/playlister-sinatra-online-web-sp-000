<h2>Artist's Songs:</h2>

<ul>
  <% @artist.songs.each do |song| %>
    <% binding.pry %>

  <a href="/artists/<%=song.name.slug%>"><li><h2><%= song.name %></h2></li></a>
  <% end %>
</ul>


<h2>Artist's Genres:</h2>

<ul>
  <% @artist.genres.each do |genre| %>
<% binding.pry %>
  <a href="/artists/<%=genre.name.slug%>"><li><h2><%= genre.name %></h2></li></a>
  <% end %>
</ul>
